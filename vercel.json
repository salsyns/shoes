{
    "rewrites": [
      {
        "src": "^/cart$",
        "dest": "/cartPage.html"
      },
      {
        "src": "^/login$",
        "dest": "/login.html"
      },
      {
        "src": "^/$",
        "dest": "/index.html"
      },
      {
        "src": "^/checkout$",
        "dest": "/checkout.html"
      },
      {
        "src": "^/ProductDetails$",
        "dest": "/ProductDetails.html"
      },
      {
        "src": "^/products$",
        "dest": "/products.html"
      },
      {
        "src": "^/signup$",
        "dest": "/signup.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  